{
	"expo": {
		"name": "Music Player",
		"slug": "music_player_app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "dark",
		"scheme": "music-player",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#000"
		},
		"ios": {
			"supportsTablet": true,
			"infoPlist": {
				"UIBackgroundModes": ["audio"]
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#000"
			},
			"package": "com.mohamedodesu.music_player_app"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true,
			"tsconfigPaths": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "cee853b1-74af-4e57-9bcd-6dec75d2df2f"
			}
		},
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/cee853b1-74af-4e57-9bcd-6dec75d2df2f"
		}
	}
}
