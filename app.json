{
	"expo": {
		"name": "music_player_app",
		"slug": "music_player_app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "dark",
		"scheme": "music-player",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#000"
		},
		"ios": {
			"supportsTablet": true,
			"infoPlist": {
				"UIBackgroundModes": ["audio"]
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#000"
			},
			"package": "com.mohamedodesu.music_player_app"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-build-properties",
				{
					"ios": {
						"newArchEnabled": true
					},
					"android": {
						"newArchEnabled": true
					}
				}
			]
		],
		"experiments": {
			"typedRoutes": true,
			"tsconfigPaths": true
		}
	}
}
