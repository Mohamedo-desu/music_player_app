{
  "name": "music_player_app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "build:android": "npx eas-cli build -p android --profile preview",
    "offline": "expo start --offline",
    "publish:expo": "eas update --branch preview && npx sentry-expo-upload-sourcemaps dist",
    "publish:sentry": "npx sentry-expo-upload-sourcemaps dist",
    "upgrade": "npm install expo@latest && npx expo install --fix",
    "android": "npx expo run:android",
    "test": "jest --watchAll",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "private": true,
  "dependencies": {
    "@react-native-menu/menu": "^1.1.2",
    "expo": "^51.0.28",
    "expo-blur": "^13.0.2",
    "expo-constants": "^16.0.2",
    "expo-dev-client": "^4.0.23",
    "expo-linear-gradient": "^13.0.2",
    "expo-linking": "^6.3.1",
    "expo-router": "~3.5.22",
    "expo-status-bar": "^1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-awesome-slider": "^2.5.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-image-colors": "^2.4.0",
    "react-native-loader-kit": "^2.0.8",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-track-player": "^4.1.1",
    "ts-pattern": "^5.3.1",
    "zustand": "^4.5.4",
    "expo-build-properties": "~0.12.5",
    "expo-system-ui": "~3.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "listUnknownPackages": false
      }
    }
  }
}
